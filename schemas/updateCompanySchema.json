{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "minProperties": 1,
    "additionalProperties": false,
    "properties": {
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Apple"
        ],
        "pattern": "^(.*)$"
      },
      "num_employees": {
        "$id": "#/properties/num_employees",
        "type": "integer",
        "title": "The Num_employees Schema",
        "default": 0,
        "examples": [
          3000
        ]
      },
      "description": {
        "$id": "#/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "Steve Jobs' lil' baby girl grow big now."
        ],
        "pattern": "^(.*)$"
      },
      "logo_url": {
        "$id": "#/properties/logo_url",
        "type": "string",
        "title": "The Logo_url Schema",
        "default": "",
        "examples": [
          "https://www.apple.com/ac/structured-data/images/knowledge_graph_logo.png"
        ],
        "pattern": "(https?:\/\/(?:www\\.|(?!www))[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|www\\.[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|https?:\/\/(?:www\\.|(?!www))[a-zA-Z0-9]+\\.[^\\s]{2,}|www\\.[a-zA-Z0-9]+\\.[^\\s]{2,})",
        "format": "regex"
      },
      "_token": {
        "$id": "#/properties/_token",
        "type": "string",
        "title": "The _token Schema",
        "default": "",
        "examples": [
          "asajasdfhasdfow"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
